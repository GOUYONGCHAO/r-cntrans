#: console.c:2220
msgid "Scrolling.\n"
msgstr "滚动：\n"

#: console.c:2221
msgid "  Keyboard: PgUp, PgDown, Ctrl+Arrows, Ctrl+Home, Ctrl+End,\n"
msgstr "  键盘：PgUp，PgDown，Ctrl+方向键，Ctrl+Home，Ctrl+End；\n"

